{
  "name": "@akadenia/azure-storage",
  "version": "1.4.1",
  "description": "Microsoft Azure storage helper methods",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "Azure",
    "Blob Storage",
    "Queue Storage",
    "Table Storage",
    "akadenia"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "test": "jest",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,json,yml}\"",
    "lint": "prettier --check \"./**/*.{ts,tsx,js,jsx,json,yml}\"",
    "prepare": "husky",
    "version": "git add -A src",
    "updateLink": "npm run build && npm rm @akadenia/azure-storage -g && npm link",
    "generate:docs": "ts-node generate-docs.ts"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akadenia/AkadeniaAzureStorage.git"
  },
  "author": "Akadenia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akadenia/AkadeniaAzureStorage/issues"
  },
  "homepage": "https://github.com/akadenia/AkadeniaAzureStorage#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@azure/data-tables": "^13.2.2",
    "@azure/logger": "^1.1.4",
    "@azure/storage-blob": "^12.24.0",
    "@azure/storage-queue": "^12.23.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@jest/globals": "^29.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@types/jsdoc-to-markdown": "^7.0.6",
    "@types/node": "^20.14.15",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jsdoc-to-markdown": "^8.0.3",
    "prettier": "^3.3.3",
    "semantic-release": "22.0.12",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
